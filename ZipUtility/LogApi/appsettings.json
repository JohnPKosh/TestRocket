{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "SlimFile": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "LogApi": "Trace"
      },
      "Folder": "./logs",
      "FileExtension": ".log",
      "MaxFileSizeInMB": 5,
      "LogSizeCheckInterval": 200
    },
    "Console": {
      "LogLevel": {
        "Default": "Error",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "LogApi": "Error"
      }
    }
  },
  "AllowedHosts": "*"
}
