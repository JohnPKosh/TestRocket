{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "LogApi": "Trace"
      },
      "Folder": "./bin/Logs",
      "FileExtension": ".log"
    },
    "Console": {
      "LogLevel": {
        "Default": "Error",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "LogApi": "Error"
      }
    }
  },
  "AllowedHosts": "*"
}
